{
  "name": "takepips-backend",
  "version": "1.0.0",
  "description": "Backend para receber sinais de trading do indicador TakePips",
  "scripts": {
    "dev": "tsx scripts/dev-server.ts",
    "dev:vercel": "vercel dev",
    "build": "echo 'Vercel compila automaticamente serverless functions'",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext .ts",
    "migrate": "node -r dotenv/config scripts/migrate.js",
    "generate:icons": "node scripts/generate-icons.js && node scripts/generate-favicon.js",
    "generate:icons:png": "node scripts/generate-icons.js",
    "generate:favicon": "node scripts/generate-favicon.js",
    "generate:mobile": "node scripts/generate-mobile-assets.js",
    "generate:all": "npm run generate:icons && npm run generate:mobile"
  },
  "dependencies": {
    "@neondatabase/serverless": "^0.9.0",
    "@stackframe/js": "^2.8.48",
    "@supabase/supabase-js": "^2.78.0",
    "web-push": "^3.6.6",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/web-push": "^3.6.4",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "@vercel/node": "^3.0.7",
    "dotenv": "^16.3.1",
    "eslint": "^8.54.0",
    "sharp": "^0.33.5",
    "to-ico": "^1.1.5",
    "tsx": "^4.7.0",
    "typescript": "^5.3.2",
    "vercel": "^32.5.2"
  },
  "engines": {
    "node": ">=18.x"
  }
}
